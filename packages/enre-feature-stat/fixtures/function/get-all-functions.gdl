// script

use coref::javascript::*

fn out(
    filePath: string,
    functionOid: int,
    functionName: string,
    functionStartLine: int,
    functionStartColumn: int,
    functionNodeType: string,
) -> bool {
    let (db = JavascriptDB::load("coref_javascript_src.db")) {
        for (func in FunctionLikeDeclaration(db), file in File(db), sk in SyntaxKind()) {
            if (file = func.getLocation().getFile() &&
                filePath = file.getRelativePath() &&
                functionOid = func.id &&
                functionName = func.getName() &&
                functionStartLine = func.getLocation().getStartLineNumber() &&
                functionStartColumn = func.getLocation().getStartColumnNumber() &&
                sk.id = func.getKind() &&
                functionNodeType = sk.getName()
            ) {
                return true
            }
        }
    }
}

fn main() {
    output(out())
}
